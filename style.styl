#about-main{
width:100%;
height:100%;
}
#about-main .text-description{
width:80%;
height:15%;

margin:0 auto;

text-align:center;
}
#about-main .text-description h1{
margin:5% 0 2% 0;

font-size:3max;
font-weight:bolder;

color:rgb(76,76,81);
}
#about-main .text-description p{
font-size:1.4vmax;
line-height:130%;

color:rgb(30,30,30);
}
#about-main .wrap-img{
width:100%;
height:23%;
display:flex;
overflow:hidden;

box-shadow:0 0 3vh 1vh rgba(190,192,192,.8);
}
#about-main .wrap-img:after{
width:100%;
height:100%;

position:absolute;
top:0;
left:0;

background:rgba(0,0,0,.1);

content:'';
}
#about-main .dynamic-img{
width:33%;
height:98%;
overflow:hidden;

box-sizing:border-box;
margin:auto;

border:.5vh solid white;
}
#about-main .dynamic-img img{
height:100%;

box-sizing:border-box;
}
/*
========================================================
======                about-category            ========
========================================================
*/
#about-main .wrap-about-category{
width:100%;
height:80%;

margin-top:5%;
}
/*======================navigation*/
#about-main nav{
width:70%;
height:5%;
display:flex;

margin:0 auto;

text-align:center;
}
#about-main nav > div{
width:19%;
height:80%;
display:flex;

margin:auto;
padding:.2% 1%;

font-size:1.2vmax;

color:rgb(26,41,79);
border-radius:1.5vh;
background:rgb(92,139,231);

cursor:pointer;
transition:all .2s ease-in;

justify-content:center;
align-items:center;
}
#about-main nav > div:hover{
color:white;
background:rgba(0,107,255,1);
box-shadow:0 0 .5vmax 0 rgba(0,107,255,1);
}

/*=================the block side =================*/
#about-main .about-category{
width:100%;
height:100%;

margin:3% 0 5% 0;
}
#about-main .about-category > div{
width:100%;
height:100%;
display:none;

position:absolute;
top:0;
left:0;
box-sizing:border-box;
padding:4% 0;

color:white;
background:rgba(31,40,51,1);
}
#about-main .about-category > div:nth-child(1){
display:block;
}
#about-main .about-category .title{
font-size:2vmax;
text-align:center;
}

/*
========================================================
======                comment: SELF-GOVERNMENT  ========
========================================================
*/
#about-main .about-category > .self-government{
height:130%;
}
.self-government #epigraph{
width:25%;

position:absolute;
top:5%;
right:3%;

font-size:1vmax;
}
.self-government #epigraph .autor{
text-align:right;
}
.self-government .table{
width:70%;
height:80%;

margin:7% auto;
}
.self-government .table .main-govern{
width:30%;

margin:2% auto;
padding:1% 4%;

font-size:1.3vmax;
text-align:center;

border-radius:3vmax;
background:rgb(92,139,231);
}
.self-government .table .name{
margin-top:1%;

font-size:1.1vmax;

color:rgb(252,204,2);
}
/*
========================================================
======                comment: RATING block     ========
========================================================
*/
#about-main .about-category > div.rating{
height:130%;
}
#about-main .rating .table{
width:70%;
height:80%;
display:flex;

margin:2.5% auto 2% auto;

background:white;

counter-reset:item;

flex-wrap:wrap;
perspective:3000px;
justify-content:space-around;
align-items:center;
}
.rating .table .best-pupil{
width:15%;
height:15%;

top:0;
left:0;

cursor:pointer;
transition:all .35s ease-in;
transform-style:preserve-3d;

perspective:900px;
}
.rating .table .best-pupilHover{
width:22%;
height:22%;

z-index:10;
top:4.5%;
left:3.5%;
margin-top:-8%;
margin-left:-7%;

transform:rotateY(180deg);
}
.rating .table .best-pupil:after{
width:11%;
min-width:20px;
height:11%;

position:absolute;
position:absolute;
right:-4%;
bottom:-4%;
padding:.5vmin;

text-align:center;

border-radius:10vh;
background:rgb(0,107,255);

content:counter(item) ' ';
counter-increment:item;
transform:translate3d(0,0,.1vh);
}

.rating .table .best-pupil .front-side{
width:100%;
height:100%;

position:absolute;
top:0;
left:0;
box-sizing:border-box;

border:.5vh solid rgba(31,40,51,.5);
background:rgb(31,40,51);
background-size:cover !important;

transform:translate3d(0,0,0);
transform-style:preserve-3d;
}
.rating .table .best-pupil:nth-child(5) .front-side{
background:url(img/about/rating/5.jpg);
}
.rating .table .best-pupil .backside{
width:100%;
height:100%;

position:absolute;
top:0;
left:0;

text-align:center;

background:rgb(0,107,255);

transform:translate3d(0,0,-.1vh) rotateY(180deg);
transform-style:preserve-3d;
}
.rating .table .best-pupil .name{
margin:4% 0 2% 0;

font-size:1.5vmax;
}
.rating .table .best-pupil .description{
font-size:2.3vh;
}
.ghost{
background:rgba(0,0,0,.1);
}
/*
========================================================
======				comment: Page News			========
========================================================
*/
#news-wrap{
	.page-title{
		width:100%;
		height:10%;
		background:rgba(black,.005);
		box-shadow:0 .04vmax 0 0 rgba(black,.5);
		h1{
			text-align:center;
			font-size:3.2vmax;
			color:rgba(black,.7);
			margin:0% 0 0 0;
			top:30%;
			font-weight:900;
		}
	}
	.newsList-wrap{
		width:100%;
		height:100%;
		.news-item{
			text-align:center;
			width:100%;
			height:22%;
			box-sizing:border-box;
			padding:0 15%;
			box-shadow:0 .04vmax 0 0 rgba(black,.4);
			p{	
				&.date{
					top:17%;
					font-size:1.1vmax;
					color:rgba(30,40,30,.9)
				}
			}
			h1.title{
				top 22%
				font-size 2.6vmax
				font-weight bold
				padding:0 35%;
				a{
					width:auto;
					transition: all .2s ease;
					color:rgba(18,65,157,.8);
					&:hover{
						color:darken(@color,1);
						text-shadow 0 0 .1vmax darken(@color,5);
					}
				}
			}
			.main-text{
				top:28%;
				font-size:1vmax;
				font-family:'Open Sans',inherit;
				font-weight:1;
			}
		}
	}
}

/*
========================================================
======				comment: Page AdminGroup	========
========================================================
*/
#adminGroup-wrap{
	.adminMem-wrap{
		width:70%;
		margin:6% auto;
		height:100%;
		.adminMem{
			width:100%;
			height:15%;
			margin-bottom:2%;
			.photo{
				width:21%;
				height:100%;
				float:left;
				border-radius:10vmax;
				background-size:cover !important;
			}
			&:nth-child(1) .photo{
				background:url(img/adminGroup/director.jpg);
			}
			for i in 2 .. 5{
				&:nth-child( {i} ) .photo{
					background:url('img/adminGroup/deputy-'i+-1'.jpg');
				}
			}
			.discription{
				float:left;
				width:70%;
				height:100%;
				margin:3% 0 0 6%;
				.name{
					font-size:1.7vmax;
				}
				p{
					font-size:1.1vmax;
				}
			}
		}
	}
}
/*
========================================================
======				comment: Page History		========
========================================================
*/
#cont3d > li > #history-main{
	background url(img/history/bg.png) no-repeat center bottom
	background-color:rgba(225,225,225,.45)
	height 190%
	.historyStep-wrap{
		background rgba(white,.5)
		width 80%
		height 95%
		margin 3% auto
		font-weight 500
		img{
			float left
			margin-right 5%
		}
		.general{
			width 85%
			margin 0 7%
			text-align:center;
			.title{
				font-size:2.7vmax;
				margin-bottom:3%;
			}
			.descrip{
				font-size:1.4vmax;
				text-align left
				line-height 140%
			}
		}
		.yearStep{
			width:100%;
			margin:2% 0;
			background:rgba(0,0,0,.01);
			padding:2%;
			line-height:150%;
			box-sizing:border-box;
			cursor:pointer;
			transition: all .5s ease;
			font-size:1.5vmax;
			&:hover{
				background:rgba(225,225,225,.45);
			};
			img{
				width:30%;
				border-bottom: 3vmax solid transparent
			}
			#special{
				margin-bottom:5%;
			}
			.year{
				font-size:2vmax;
				font-weight bold
				color rgba(92,139,231,1)
				margin:1% 0;
				text-align center
			}
		}
	}
}
/*
========================================================
======				comment: Page DSD   		========
