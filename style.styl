/*
========================================================
======				comment: navigation 		========
========================================================
*/
#nav-panel{
	width:100%;
	height:10%;
	
	position:fixed;
	z-index:2;

	font-family:RobotoRegular,Arial;

	perspective:2000px;
	&:hover .mainNav-wrap{
		transform:rotateX(25deg);
	}
	.mainNav-wrap{
		background:rgb(92,139,231);
		box-shadow:0 0 2vh 0 black;
		transition: all .2s ease;
		transform-origin:0 0;
		width:100%;
		height:100%;
		
		#logo{
			width:25%;
			height:100%;

			position:absolute;
			top:0;
			left:4%;

			cursor:pointer;
			img{
				width:auto;
				height:95%;
				float:left;

				top:2.5%;
				margin-right:2%;
			}
			h1{
				margin:3% 0 0 0;

				font-size:2.2vmax;
				font-weight:normal;

				color:white;

				text-transform:uppercase;
			}
			#home{
				margin:-1.5% 0 0 47%;

				font-size:.95vmax;
				font-weight:bold;

				color:rgba(230,230,230,1);
			}
		}
		nav{
			width:75%;
			height:60%;
			display:flex;
			justify-content:space-around;

			top:25%;
			left:25%;
			> div{
				box-sizing:border-box;
				width:15%;
				height:100%;
				font-size:1.1vmax;
				text-align: center;
				padding-top:.5%;
				color:rgb(250,250,250);
				border:.2vh solid transparent;
				border-radius:.9vh;

				cursor:pointer;
				transition:all .2s ease-in;
				text-transform:uppercase;
				&:hover{
					border:.2vh solid white;
					background:rgb(92,139,231);
					height:400%;
				}
				&:nth-child(2){
					width:13%;
					font-size:1vmax;
					height:100% !important;
				}
				&:nth-child(3){
					width:13%;
					margin-left:-2%;
					font-size:1vmax;
				}
				&:nth-child(4){
					width:18%;
					font-size:1vmax;
				}
				&:hover .name{
					transform:scale(.7, .5);
				}
				&:hover .ctg{
					transform:scale(1) translateY(1vmax);
					>*{
						padding:6% 0;
					}
				}
				.name{
					margin-top:4%;
					transition: all .4s linear;
					font-size:inherit;
				}
				#news-name{
					transform:none !important;
				}
				.ctg{
					transform:scale(1, .01);
					transform-origin:0 0;
					text-align:center;
					font-size:1vmax;

					transition: all .4s ease;
					>*{
						&:hover{
							background:white;
							color:rgb(50,50,50);
							transition: all .3s linear;
						}
					}
				}
			}
			.buttonActive{
			border:.2vh solid white;
			}
		}
	}
}

