ul#cont3d{
	width:100%;
	height:100%;
	display:none;

	position:fixed;
	top:0;
	left:0;

	opacity:1;

	transition:all .9s cubic-bezier(.785, .135, .15, .86);
	// transform:translate3d(0,-600vh,-2000vh);
	transform-style:preserve-3d;

	perspective:50000000px;
	li{
		width:100%;
		height:100%;
		overflow:auto;

		position:absolute;
		top:0;
		left:0;
		box-sizing:border-box;

		background:white !important;
		box-shadow:inset 0 0 3vh 1vh rgba(40,45,51,1);

		list-style:none;
		transition:all .3s ease;
		perspective:900px;
		> div{
			width:100%;
			height:100%;
			transition:all .4s ease-in;
			position:absolute;
			top:0;
			left:0;
			transform:scale(.1);
			opacity:0;
			&.ctgSideActive{
				transform:scale(1) !important;
				z-index:4 !important;
				opacity:1 !important;
			}
		}
		&.front{

		}
		&.left{
			left:-100%;

			transform:rotateY(-90deg);
			transform-origin:100% 50%;

		}
		&.right{
			left:100%;

			transform:rotateY(90deg);
			transform-origin:0 50%;
		}
		&.top{
			transform:rotateX(-90deg);
			transform-origin:50% 0;
		}
		&.bottom{
			transform:rotateX(90deg);
			transform-origin:50% 100%;
		}
		&.under{
			transform-origin:50% 50%;
		}
	}
	.nonShadow{
		box-shadow inset 0 0 1vh 0 rgba(40,45,51,1);
	}
}
.contHidden{
opacity:0 !important;

transform:translate3d(0,0,0) !important;
}

